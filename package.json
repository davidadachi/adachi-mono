{
  "name": "goldfinch-protocol",
  "version": "0.1.0",
  "description": "A lending protocol built on the blockchain",
  "main": "index.js",
  "scripts": {
    "start": "REACT_APP_HARDHAT_FORK=mainnet HARDHAT_FORK=mainnet npx concurrently -n \"blockchain,web,server\"  \"npx hardhat node --reset --export-all client/config/deployments_dev.json\" \"cd client && npm start\" \"HARDHAT_NETWORK=localhost node autotasks/server.js\" -k -c \"cyan, magenta, green\"",
    "no-gasless-start": "REACT_APP_DISABLE_GASLESS=true REACT_APP_HARDHAT_FORK=mainnet HARDHAT_FORK=mainnet npx concurrently -n \"blockchain,web,server\"  \"npx hardhat node --reset --export-all client/config/deployments_dev.json\" \"cd client && npm start\" \"HARDHAT_NETWORK=localhost node autotasks/server.js\" -k -c \"cyan, magenta, green\"",
    "local-start": "npx concurrently -n \"blockchain,web,server\"  \"npx hardhat node --reset --export-all client/config/deployments_dev.json\" \"cd client && npm start\" \"HARDHAT_NETWORK=localhost node autotasks/server.js\" -k -c \"cyan, magenta, green\"",
    "test": "NODE_ENV=test npx hardhat test --show-stack-traces",
    "mainnet-test": "HARDHAT_FORK=mainnet NODE_ENV=test npx hardhat test test/MainnetForking.test.js --show-stack-traces",
    "pre-push": "cd client && npm run pre-push",
    "pre-commit": "npx lint-staged && cd client && npm run pre-commit",
    "lint": "npx eslint . --fix && npx prettier . --write && npx solhint contracts/**/*.sol --fix",
    "docs": "npx solidity-docgen && rm -rf docs/test",
    "verify": "npx hardhat etherscan-verify --api-key DQUC8Y678J5RN5P7XE9RT91SWI7SSEDD53 --solc-input --network",
    "deploy": "npx hardhat deploy --show-stack-traces --export-all client/config/deployments.json --network",
    "upgrade-protocol": "npm run docs && npx hardhat deploy --export-all client/config/deployments.json --tags upgrade_protocol --network",
    "update-configs": "npx hardhat run ./blockchain_scripts/updateConfigs.js --network",
    "upgrade-multisig": "npx hardhat run ./blockchain_scripts/upgrade.js --network",
    "autotask-update": "node autotasks/deployAutoTasks.js",
    "advance-time": "npx hardhat run ./blockchain_scripts/advanceTime.js --network localhost"
  },
  "lint-staged": {
    "**!(client)/*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.sol": [
      "prettier --write",
      "solhint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run pre-push",
      "pre-commit": "npm run pre-commit"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenZeppelin/starter-kit"
  },
  "keywords": [
    "openzeppelin",
    "truffle",
    "react",
    "dapp",
    "infura",
    "metamask",
    "web3",
    "ganache",
    "web3js",
    "ethereum"
  ],
  "author": "Ramon Recuero <ramon@dapis.io>, Igor Yalovoy <to@ylv.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OpenZeppelin/starter-kit/issues"
  },
  "homepage": "https://github.com/OpenZeppelin/starter-kit/blob/stable/README.md",
  "dependencies": {
    "@opengsn/gsn": "^2.1.0",
    "@openzeppelin/contracts-ethereum-package": "^3.0.0",
    "@openzeppelin/upgrades": "^2.8.0",
    "bn.js": "^5.1.3",
    "contract-proxy-kit": "^2.2.0-alpha.1",
    "defender-autotask-client": "^1.4.1",
    "defender-relay-client": "^1.3.1",
    "dotenv": "^8.2.0",
    "eth-sig-util": "^3.0.0",
    "ethereumjs-util": "^7.0.8",
    "ethers": "^5.0.23",
    "react-router-dom": "^5.2.0",
    "truffle-hdwallet-provider": "^1.0.17"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/cli": "^2.6.0",
    "@openzeppelin/test-helpers": "^0.5.6",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/is-odd": "^3.0.0",
    "builtin-modules": "^3.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-bn": "github:OpenZeppelin/chai-bn#v0.2.1",
    "concurrently": "^5.3.0",
    "defender-admin-client": "^1.4.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-flowtype": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "ganache-cli": "^6.10.2",
    "hardhat": "^2.0.8",
    "hardhat-deploy": "^0.7.0-beta.46",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "mocha": "^8.1.3",
    "mocha-each": "^2.0.1",
    "prettier": "^2.1.2",
    "prettier-plugin-solidity": "^1.0.0-alpha.59",
    "rollup": "^2.33.3",
    "solhint": "^3.2.1",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-docgen": "^0.5.7",
    "truffle-contract-size": "^2.0.0"
  }
}
