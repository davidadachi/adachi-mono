{
  "name": "goldfinch-protocol",
  "version": "0.1.0",
  "description": "A lending protocol built on the blockchain",
  "main": "index.js",
  "scripts": {
    "start": "npx concurrently -n \"blockchain,web\"  \"npx buidler node --reset --export-all client/config/deployments_dev.json\" \"cd client && npm start\" -k -c \"cyan, magenta\"",
    "test": "NODE_ENV=test npx buidler test --show-stack-traces",
    "pre-push": "cd client && npm run pre-push",
    "pre-commit": "npx lint-staged && cd client && npm run pre-commit",
    "lint": "npx eslint . --fix && npx prettier . --write && npx solhint **/*.sol --fix",
    "verify": "npx buidler etherscan-verify --api-key DQUC8Y678J5RN5P7XE9RT91SWI7SSEDD53 --solc-input --network",
    "deploy": "npx solidity-docgen && rm -rf docs/test && npx buidler deploy --show-stack-traces --export-all client/config/deployments.json --network",
    "upgrade-protocol": "npx solidity-docgen && rm -rf docs/test && npx buidler deploy --export-all client/config/deployments.json --tags upgrade_protocol --network",
    "update-configs": "npx buidler run ./blockchain_scripts/updateConfigs.js --network"
  },
  "lint-staged": {
    "**!(client)/*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.sol": [
      "prettier --write",
      "solhint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run pre-push",
      "pre-commit": "npm run pre-commit"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenZeppelin/starter-kit"
  },
  "keywords": [
    "openzeppelin",
    "truffle",
    "react",
    "dapp",
    "infura",
    "metamask",
    "web3",
    "ganache",
    "web3js",
    "ethereum"
  ],
  "author": "Ramon Recuero <ramon@dapis.io>, Igor Yalovoy <to@ylv.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OpenZeppelin/starter-kit/issues"
  },
  "homepage": "https://github.com/OpenZeppelin/starter-kit/blob/stable/README.md",
  "dependencies": {
    "@openzeppelin/contracts-ethereum-package": "^3.0.0",
    "@openzeppelin/upgrades": "^2.8.0",
    "bn.js": "^5.1.3",
    "dotenv": "^8.2.0",
    "ethers": "^5.0.14",
    "react-router-dom": "^5.2.0",
    "truffle-hdwallet-provider": "^1.0.17"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.7",
    "@nomiclabs/buidler-ethers": "^2.0.0",
    "@nomiclabs/buidler-truffle5": "^1.3.4",
    "@nomiclabs/buidler-web3": "^1.3.4",
    "@openzeppelin/cli": "^2.6.0",
    "@openzeppelin/test-helpers": "^0.5.6",
    "buidler-deploy": "https://github.com/goldfinch-eng/buidler-deploy.git#512817b7e7d5f7c6523aa1c173a35870672d3e55",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-bn": "github:OpenZeppelin/chai-bn#v0.2.1",
    "concurrently": "^5.3.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-prettier": "^3.1.4",
    "ganache-cli": "^6.10.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "mocha": "^8.1.3",
    "mocha-each": "^2.0.1",
    "prettier": "^2.1.2",
    "prettier-plugin-solidity": "^1.0.0-alpha.59",
    "solhint": "^3.2.1",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-docgen": "^0.5.7",
    "truffle-contract-size": "^2.0.0"
  }
}
