{
  "name": "@goldfinch-eng/client2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare-data": "ts-node scripts/populate-contract-addresses.ts && ts-node scripts/cache-cms-data.ts && yarn gen-contract-types && yarn codegen",
    "dev": "yarn prepare-data && ts-node scripts/copy-gfi-grant-files.ts && next dev -p 3001",
    "build": "yarn prepare-data && next build",
    "start": "next start -p 3001",
