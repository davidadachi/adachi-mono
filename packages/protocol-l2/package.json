    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "bignumber.js": "^9.0.1",
    "bn.js-typings": "^1.0.1",
    "builtin-modules": "^3.1.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-bn": "^0.3.1",
    "concurrently": "^5.3.0",
    "defender-admin-client": "^1.4.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.28.0",
